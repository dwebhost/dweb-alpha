{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/upload-srv/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/upload-srv/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/upload-srv",
  "projects": {
    "deploy-srv": {
      "type": "application",
      "root": "apps/deploy-srv",
      "entryFile": "main",
      "sourceRoot": "apps/deploy-srv/src",
      "compilerOptions": {
        "tsConfigPath": "apps/deploy-srv/tsconfig.app.json"
      }
    },
    "upload-srv": {
      "type": "application",
      "root": "apps/upload-srv",
      "entryFile": "main",
      "sourceRoot": "apps/upload-srv/src",
      "compilerOptions": {
        "tsConfigPath": "apps/upload-srv/tsconfig.app.json"
      }
    }
  }
}