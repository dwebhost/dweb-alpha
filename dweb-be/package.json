    "axios": "^1.7.9",
    "bullmq": "^5.41.3",
    "class-validator": "^0.14.1",
    "fs-extra": "^11.3.0",
    "ioredis": "^5.5.0",
    "multer": "1.4.5-lts.1",
    "pinata-web3": "^0.5.4",
    "reflect-metadata": "^0.2.2",
